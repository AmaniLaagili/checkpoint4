

FUNCTION insertionSort(arr : ARRAY_OF INTEGER) : ARRAY_OF INTEGER
VAR
    i,key,j :INTEGER
BEGIN
    FOR i FROM 1 TO len(arr[i]) STEP 1  DO
        key := arr[ i ];  
        j:=i-1
      END_FOR
      WHILE (j > 0 & arr[i]> key) DO
          arr[i+1]:=arr[ i ]
          j := j â€” 1
      END_WHILE
        arr[j+1] = key
    RETURN arr[i] ;

END

